{
  "name": "windmill/laravel-windmill-queue",
  "description": "Custom Laravel queue driver using Windmill protocol over HTTP. Stores jobs in MySQL. Consumer is triggered via HTTP listener (Windmill).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Baba Ecip",
      "email": "itdev.septian@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/queue": "^5.7",
    "illuminate/support": "^5.7",
    "illuminate/contracts": "^5.7",
    "illuminate/redis": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Windmill\\Queue\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Windmill\\Queue\\WindmillQueueServiceProvider"
      ]
    }
  },
  "keywords": [
    "laravel",
    "queue",
    "windmill",
    "driver",
    "http",
    "mysql"
  ],
  "support": {
    "email": "itdev.septian@gmail.com",
    "source": "https://github.com/babaecip/laravel-windmill-queue"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
